namespace DirectX.Windows.Winuser
{
	/**
	 * Contains predefined cursors.
	*/
	public static class DefaultCursors
	{
		/**
		 * Standard arrow and small hourglass.
		 * IDC_APPSTARTING
		*/
		public static readonly char16* AppStarting = MAKEINTRESOURCEW(32650);
		/**
		 * Standard arrow.
		 * IDC_ARROW
		*/
		public static readonly char16* Arrow = MAKEINTRESOURCEW(32512);
		/**
		 * Crosshair.
		 * IDC_CROSS
		*/
		public static readonly char16* Crosshair = MAKEINTRESOURCEW(32515);
		/**
		 * Hand.
		 * IDC_HAND
		*/
		public static readonly char16* Hand = MAKEINTRESOURCEW(32649);
		/**
		 * Arrow and question mark.
		 * IDC_HELP
		*/
		public static readonly char16* Help = MAKEINTRESOURCEW(32651);
		/**
		 * I-beam.
		 * IDC_IBEAM
		*/
		public static readonly char16* IBeam = MAKEINTRESOURCEW(32513);
		/**
		 * Obsolete for applications marked version 4.0 or later.
		 * IDC_ICON
		*/
		public static readonly char16* Icon = MAKEINTRESOURCEW(32641);
		/**
		 * Slashed circle.
		 * IDC_NO
		*/
		public static readonly char16* No = MAKEINTRESOURCEW(32648);
		/**
		 * Obsolete for applications marked version 4.0 or later. Use IDC_SIZEALL.
		 * IDC_SIZE
		*/
		public static readonly char16* Size = MAKEINTRESOURCEW(32640);
		/**
		 * Four-pointed arrow pointing north, south, east, and west.
		 * IDC_SIZEALL
		*/
		public static readonly char16* SizeAll = MAKEINTRESOURCEW(32646);
		/**
		 * Double-pointed arrow pointing northeast and southwest.
		 * IDC_SIZENESW
		*/
		public static readonly char16* SizeNortheastSouthwest = MAKEINTRESOURCEW(32643);
		/**
		 * Double-pointed arrow pointing north and south.
		 * IDC_SIZENS
		*/
		public static readonly char16* SizeNorthSouth = MAKEINTRESOURCEW(32645);
		/**
		 * Double-pointed arrow pointing northwest and southeast.
		 * IDC_SIZENWSE
		*/
		public static readonly char16* SizeNorthwestSoutheast = MAKEINTRESOURCEW(32642);
		/**
		 * Double-pointed arrow pointing west and east.
		 * IDC_SIZEWE
		*/
		public static readonly char16* SizeEastWest = MAKEINTRESOURCEW(32644);
		/**
		 * Vertical arrow.
		 * IDC_UPARROW
		*/
		public static readonly char16* UpArrow = MAKEINTRESOURCEW(32516);
		/**
		 * Hourglass.
		 * IDC_WAIT
		*/
		public static readonly char16* Wait = MAKEINTRESOURCEW(32514);
	}
}
